/***********************************************************************
 * Defines setupExplorer() method for ChromosomeExplorer
 * The file was automatically generated by aroma.affymetrix.
 * Do not edit this file!
 ***********************************************************************/
function setupExplorer(explorer) {
  /* Detected chip types */
  explorer.setChipTypes(new Array(<%=paste("'", chipTypes, "'", sep="", collapse=", ")%>));

  /* Samples */
  explorer.setSamples(new Array(<%=paste("'", samples, "'", sep="", collapse=", ")%>));
  explorer.setSampleAliases(new Array(<%=paste("'", sampleLabels, "'", sep="", collapse=", ")%>));
 
  /* Detected sets */
  explorer.setSets(new Array(<%=paste(paste("'", sets, "'", sep=""), collapse=", ")%>));

  /* Chromosomes */
  explorer.setChromosomes(new Array(<%=paste("'", chromosomeLabels, "'", sep="", collapse=", ")%>));

  /* Zooms */
  explorer.setScales(new Array(<%=paste(zooms, collapse=", ")%>));

  /* Starting view */
  explorer.chromosomeIdx = 22-1;
  explorer.scaleIdx = 2;

  return(false);
}
