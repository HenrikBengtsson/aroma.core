/***********************************************************************
 * Sets onLoad() method for ChromosomeExplorer v3.3
 * The file was automatically generated by aroma.affymetrix.
 * Do not edit this file!
 ***********************************************************************/
explorer.onLoad = function() {
  /* Detected chip types */
  this.setChipTypes(new Array(<%=paste("'", chipTypes, "'", sep="", collapse=", ")%>));

  /* Samples */
  this.setSamples(new Array(<%=paste("'", samples, "'", sep="", collapse=", ")%>));
  this.setSampleAliases(new Array(<%=paste("'", gsub("[,].*$", "", samples), "'", sep="", collapse=", ")%>));
  this.setSampleAliases(new Array(<%=paste("'", sampleLabels, "'", sep="", collapse=", ")%>));
 
  /* Detected sets */
  this.setSets(new Array(<%=paste(paste("'", sets, "'", sep=""), collapse=", ")%>));

  /* Chromosomes */
  this.setChromosomes(new Array(<%=paste("'", chromosomeLabels, "'", sep="", collapse=", ")%>));

  /* Zooms */
  this.setScales(new Array(<%=paste(zooms, collapse=", ")%>));

  /* Starting view */
  this.chromosomeIdx = 22-1;
  this.scaleIdx = 2;

  return(false);
}
